<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.gentleflo.memo.post.dao.PostDAO">
 	<insert id="insertPost" parameterType="map">
 		INSERT INTO
 			`post`
 		(
 			`userId`
 			, `subject`
 			, `content`
 			, `imagePath`
 			, `createdAt`
 			, `updatedAt`
 		) VALUES (
 			#{userId}
 			, #{subject}
 			, #{content}
 			, #{imagePath}
 			, now()
 			, now() 
 		)

 	</insert>
 </mapper>